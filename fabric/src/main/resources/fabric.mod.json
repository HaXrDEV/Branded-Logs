{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "HaXr"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/branded-logs",
    "sources": "https://github.com/HaXrDEV/Branded-Logs",
    "issues": "https://github.com/HaXrDEV/Branded-Logs/issues"
  },
  "license": "LGPL 3.0",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.haxr.brandedlogs.fabric.BrandedLogsFabric"
    ],
    "modmenu": ["dev.haxr.brandedlogs.fabric.config.ModMenuIntegration"]
  },
  "mixins": [
    "common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15",
    "minecraft": "${minecraft}",
    "cloth_config": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/Kss5gBgeDA"
      }
    },
    "mc-publish": {
      "dependencies": [
        "cloth_config(required){modrinth:cloth-config,curseforge:cloth-config}"
      ]
    }
  }
}